{
    "name": "andrazero121/api-resource-typer",
    "description": "A Laravel package to automatically generate TypeScript interfaces from API Resource Controllers",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AndraZero121\\ApiResourceTyper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "AndraZero121", 
            "email": "diandraanursasyabandira@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndraZero121\\ApiResourceTyper\\Providers\\ApiResourceTyperServiceProvider"
            ]
        }
    }
}
